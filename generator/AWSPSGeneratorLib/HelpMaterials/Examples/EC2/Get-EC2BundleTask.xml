<?xml version="1.0" encoding="utf-8"?>
<examples>
  <example>
    <code>Get-EC2BundleTask -BundleId bun-12345678</code>
    <description>This example describes the specified bundle task.</description>
  </example>
  <example>
    <code>$filter = New-Object Amazon.EC2.Model.Filter
$filter.Name = "state"
$filter.Values = @( "complete", "failed" )

Get-EC2BundleTask -Filter $filter
    </code>
    <description>This example describes the bundle tasks whose state is either 'complete' or 'failed'.</description>
  </example>
</examples>