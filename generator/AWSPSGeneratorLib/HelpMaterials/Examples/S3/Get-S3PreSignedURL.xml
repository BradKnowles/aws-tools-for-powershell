<?xml version="1.0" encoding="utf-8"?>
<!--Examples for the AWS PowerShell CmdLets-->
<examples>
  <example>
    <code>Get-S3PreSignedURL -BucketName 's3testbucket' -Key 'testkey' -Expires '2023-11-16'</code>
    <description>The command returns pre-signed URL for a specified key and an expiration date.</description>
  </example>
  <example>
    <code>[Amazon.AWSConfigsS3]::UseSignatureVersion4 = $true
      Get-S3PreSignedURL -BucketName sampledirectorybucket--use1-az5--x-s3 -Key 'testkey' -Expire '2023-11-17' </code>
    <description>The command returns pre-signed URL for a Directory Bucket with specified key and an expiration date.</description>
  </example>
</examples>